{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@lib/*": ["src/library/*"],
			"@blocks/*": ["src/library/blocks/*"],
			"@layout/*": ["src/library/layout/*"],
			"@forms/*": ["src/library/forms/*"],
			"layout/*": ["src/library/layout/*"]
		}
	},
	"exclude": ["node_modules"]
}
